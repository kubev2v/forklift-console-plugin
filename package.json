{
  "name": "forklift-console-plugin",
  "version": "2.11.0",
  "description": "UI for forklift as an openshift console dynamic plugin",
  "license": "Apache-2.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "repository": "https://github.com/kubev2v/forklift-console-plugin.git",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./dist ./coverage",
    "clean:all": "npm run clean && rm -rf ./node_modules ./tmp",
    "build": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 webpack",
    "build:analyze": "ANALYZE_BUNDLE=true NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack serve --progress",
    "i18n": "i18next \"./src/**/*.{js,jsx,ts,tsx}\" ./plugin-extensions.ts [-oc] -c ./i18next-parser.config.ts",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' plugin-extensions.ts plugin-metadata.ts environment-defaults.ts --cache --cache-location .eslintcache && stylelint \"src/**/*.css\" --allow-empty-input",
    "lint:fix": "eslint . --fix && stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "lint:ia": "rm -rf node_modules/.cache/eslint-interactive && npx eslint-interactive .",
    "test:i18n": "bash ./ci/test-i18n.sh",
    "test": "TZ=UTC jest",
    "test:coverage": "TZ=UTC jest --coverage",
    "test:updateSnapshot": "TZ=UTC jest --updateSnapshot",
    "test:e2e": "npm --prefix testing run test:e2e",
    "console": "bash ./ci/start-console.sh",
    "console:stop": "bash ./ci/stop-console.sh",
    "cluster:up": "bash ./ci/deploy-all.sh",
    "cluster:delete": "bash ./ci/clean-cluster.sh",
    "e2e:cluster": "bash ./ci/deploy-cluster.sh",
    "e2e:build": "bash ./ci/build-and-push-images.sh",
    "e2e:deployCrds": "kubectl apply -f ./ci/yaml/crds/forklift",
    "e2e:console": "bash ./ci/deploy-console.sh",
    "e2e:pre-test": "npm run e2e:cluster && npm run e2e:build && npm run e2e:deployCrds && npm run e2e:console",
    "knip": "knip",
    "validate-commits": "bash ./scripts/validate-commits.sh --verbose",
    "validate-commits-range": "bash ./scripts/validate-commits.sh --range",
    "postinstall": "husky"
  },
  "dependencies": {
    "@forklift-ui/types": "1.0.7",
    "@openshift-console/dynamic-plugin-sdk": "1.8.0",
    "@patternfly/patternfly": "6.4.0",
    "@patternfly/quickstarts": "6.4.0",
    "@patternfly/react-charts": "8.4.0",
    "@patternfly/react-component-groups": "6.4.0",
    "@patternfly/react-core": "6.4.0",
    "@patternfly/react-drag-drop": "6.4.0",
    "@patternfly/react-icons": "6.4.0",
    "@patternfly/react-styles": "6.4.0",
    "@patternfly/react-table": "6.4.0",
    "@patternfly/react-tokens": "6.4.0",
    "classnames": "^2.5.1",
    "i18next": "^25.3.2",
    "immer": "^10.1.1",
    "immutable": "^5.1.1",
    "js-base64": "^3.7.7",
    "jsonpath": "^1.1.1",
    "jsrsasign": "11.1.0",
    "luxon": "^3.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^11.14.3",
    "react-linkify": "^1.0.0-alpha",
    "react-router": "5.3.x",
    "react-router-dom": "5.3.x",
    "react-router-dom-v5-compat": "^6.22.0",
    "react-tagsinput": "^3.19.0",
    "victory-area": "^37.3.6",
    "victory-axis": "^37.3.6",
    "victory-bar": "^37.3.6",
    "victory-box-plot": "^37.3.6",
    "victory-chart": "^37.3.6",
    "victory-core": "^37.3.6",
    "victory-create-container": "^37.3.6",
    "victory-cursor-container": "^37.3.6",
    "victory-group": "^37.3.6",
    "victory-legend": "^37.3.6",
    "victory-line": "^37.3.6",
    "victory-pie": "^37.3.6",
    "victory-scatter": "^37.3.6",
    "victory-stack": "^37.3.6",
    "victory-tooltip": "^37.3.6",
    "victory-voronoi-container": "^37.3.6",
    "victory-zoom-container": "^37.3.6"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.5",
    "@eslint/js": "^9.23.0",
    "@jest/globals": "^29.7.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "1.3.0",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/i18next": "^11.9.3",
    "@types/jest": "^29.5.14",
    "@types/jsonpath": "^0.2.4",
    "@types/jsrsasign": "10.5.15",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.16.6",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-tagsinput": "^3.20.6",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-interactive": "^11.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-perfectionist": "^4.10.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "i18next-parser": "^9.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.44.5",
    "lint-staged": "^15.4.3",
    "mini-svg-data-uri": "^1.4.4",
    "monaco-editor": "^0.52.2",
    "prettier": "^3.5.3",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "overrides": {
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "monaco-editor": "^0.52.2",
    "node-forge": "^1.3.2"
  }
}
